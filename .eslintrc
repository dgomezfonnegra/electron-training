{
  "env": {
    "node": true,
    "jasmine": true,
    "amd": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "Promise": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "valid-jsdoc": 2,
    "eqeqeq": 2,
    "no-empty-function": 2,
    "no-eval": 2,
    "yoda": 2,
    "block-spacing": 2,
    "indent": [2, 2],
    "no-trailing-spaces": 2,
    "spaced-comment": [2, "always"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "quotes": [2, "single"],
    "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
    "keyword-spacing": 2,
    "no-whitespace-before-property": 2,
    "space-infix-ops": 2,
    "no-multi-spaces": 2
  }
}
